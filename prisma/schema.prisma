generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  name String
}

model Favorite {
  id String @id @default(uuid())
  name String
  userId String
  user User @relation(fields: [userId], references: [id])
}
